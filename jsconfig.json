{
    "compilerOptions": {
        "baseUrl": "./src",
        "jsx": "react",
        "paths": {
            "@components/*": ["components/*"],
            "@reducers/*": ["redux/reducers/*"],
            "@stores/*": ["redux/stores/*"],
            "@types/*": ["redux/types/*"],
            "@actions/*": ["redux/actions/*"],
            "@styles/*": ["styles/*"],
            "@utils/*": ["utils/*"],
            "@pages/*": ["pages/*"],
            "@services/*": ["services/*"],
        },
    },
    "exclude": ["build", "dist"]
}
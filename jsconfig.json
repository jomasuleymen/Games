{
    "compilerOptions": {
        "baseUrl": "./src",
        "jsx": "react",
        "paths": {
            "@components/*": ["components/*"],
            "@reducers/*": ["redux/reducers/*"],
            "@store": ["redux/store.js"],
            "@actions/*": ["redux/actions/*"],
            "@styles/*": ["styles/*"],
            "@utils/*": ["utils/*"],
            "@pages/*": ["pages/*"],
            "@services/*": ["services/*"],
            "@app/*": ["app/*"],
        },
    },
    "exclude": ["node_modules", "build", "dist"]
}